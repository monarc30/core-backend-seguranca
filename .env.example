# Supabase (um conjunto por projeto SaaS; trocar por SUPABASE_URL_APP_A etc. se multi-projeto)
SUPABASE_URL=https://seu-projeto.supabase.co
SUPABASE_SERVICE_ROLE_KEY=sua-service-role-key

# JWT (validar tokens emitidos pelo Supabase Auth ou seu IdP)
JWT_SECRET=seu-jwt-secret
JWT_ISSUER=supabase

# Webhook (ex.: Stripe)
STRIPE_WEBHOOK_SECRET=whsec_...

# Auditoria
AUDIT_LOG_ENABLED=true

# Servidor
PORT=3000
